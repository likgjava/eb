<script>
var $contentSub = $('#contentSub');
$('>ul>li',$contentSub).click(function(){
		$(this).siblings('li').removeClass('selected').end().addClass('selected');
		var ch = $('#contentSub').height()-$('#contentSub>ul').height()+$(this).find('ul:first').height();
		$(this).find('ul:first').css({'overflow':'auto','height':ch+'px'})

})

$('.subnav>li>a',$contentSub).click(function(){
	   var cls = $(this).parent('li').attr('class');
	   switch(cls){
		   case 'open':
				$(this).parent('li').removeClass('open').addClass('close');
				break;
		   case 'close':
				$(this).parent('li').removeClass('close').addClass('open');
				break;
		   default:
				$('.subnav>li',$contentSub).removeClass('selected');
				$(this).parent('li').addClass('selected');
				//$('#conTitle h3').text($(this).text());
				break;
		   }
})


$('#contentSub>ul>li:first').trigger('click');
</script>